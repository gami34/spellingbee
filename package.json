{
  "name": "reactjs",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "antd": "^4.18.5",
    "axios": "^0.25.0",
    "cra-template": "1.1.3",
    "craco-less": "^2.0.0",
    "flutterwave-react-v3": "^1.2.0",
    "naija-state-local-government": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1"
  },
  "scripts": {
    "build:css": "tailwindcss -i src/styles/tailwind.css -o public/style.css",
    "watch:css": "tailwindcss -i src/styles/tailwind.css -o public/style.css -w",
    "craco:start": "craco start",
    "craco:build": "craco build",
    "start": "run-p watch:css craco:start",
    "build": "run-s build:css craco:build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint --cache --fix  \"./src/**/*.+(js|yml|yaml|json)\"",
    "format": "prettier --write \"./src/**/*.+(js|yml|yaml|json)\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.11"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,scss,json,html}": [
      "npm run format",
      "npm run lint"
    ]
  }
}
